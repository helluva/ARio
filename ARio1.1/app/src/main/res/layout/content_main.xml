<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.jakewaldner.ario11.MainActivity"
    tools:showIn="@layout/activity_main">


    <Button
        android:id="@+id/open_mario_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/gotocamera_button2"
        android:layout_below="@+id/gotocamera_button2"
        android:layout_marginTop="40dp"
        android:text="Open Mario Scene" />

    <Button
        android:id="@+id/gotocamera_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="173dp"
        android:text="Start Camera View" />

</RelativeLayout>
